#include <stdio.h>
#include<string.h>
int main()
{
char str[50]="tutorial", temp;
int i, initial, end, len;
printf(" Given String = %s \n", str);
len = strlen(str);
initial = 0;
end = len - 1;
for (i = initial; i < end; i++)
{
temp = str[i];
str[i] = str[end];
str[end] = temp;
end--;
}
printf(" Reversed String = %s ", str);
return 0;
}
